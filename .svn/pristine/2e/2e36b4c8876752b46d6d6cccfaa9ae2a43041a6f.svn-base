                trigger: 'item'  //加上这句后，图形点上同时显示其他项的数量。axis是显示所有item是显示当前
            },
            legend: {
                data: ['${sdm[0]}']    //这句为再顶部展示各条折线，点击展示与隐藏
            },

            grid: {
                left: '3%',
                right: '4%',
                bottom: '3%',
                containLabel: true
            },

            xAxis: {
                type: 'category',
                boundaryGap: false,
                data: ['${d_date[1]}', '${d_date[2]}', '${d_date[3]}', '${d_date[4]}', '${d_date[5]}',
