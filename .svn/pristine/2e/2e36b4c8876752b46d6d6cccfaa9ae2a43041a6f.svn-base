                    '${d_date[21]}', '${d_date[22]}', '${d_date[23]}', '${d_date[24]}', '${d_date[25]}',
                    '${d_date[26]}', '${d_date[27]}', '${d_date[28]}', '${d_date[29]}', '${d_date[30]}',
