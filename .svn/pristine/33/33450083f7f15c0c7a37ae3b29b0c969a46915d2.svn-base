                </ul>
            </div>

 <table class="table table-bordered" style="width:800px;height:500px;margin:auto;">

     <th colspan="17" style="text-align:center;">${send_type}波动数据</th>

     <tr>
         <td>省份</td>
         %for i in xrange(bd):

            <td>${sdm_mingc[i]}</td>

         %endfor
     </tr>
     <tr>
        <td>OneD_Rate</td>
         %for i in xrange(bd):
            %if one_bsbz[i] <= 10:
                <td>${one_dr_list[i]}</td>
            %else:
                <td bgcolor="red">${one_dr_list[i]}</td>
            %endif
         %endfor
     </tr>
     <tr>
         <td>TenD_Rate</td>
         %for i in xrange(bd):
             %if ten_bsbz[i] < 10:
                <td>${ten_tr_list[i]}</td>
            %else:
                <td bgcolor="red">${ten_tr_list[i]}</td>
            %endif
         %endfor
     </tr>

     <tr>
         <td colspan="17">
             <hr>
         </td>
     </tr>
    ##如果bd大于15表示是上发或者下发的数据。这时候才会有下面的代码。
     % if bd > 15:
          <tr>
         <td>省份</td>
         %for i in xrange(bd,cd):
            <td>${sdm_mingc[i]}</td>
         %endfor

              ##凑数用
              <td> </td>
     </tr>
     <tr>
        <td>OneD_Rate</td>
         %for i in xrange(bd,cd):
            %if one_bsbz[i] <= 10:
                <td>${one_dr_list[i]}</td>
            %else:
                <td bgcolor="red">${one_dr_list[i]}</td>
            %endif
         %endfor
         <td> </td>
     </tr>
     <tr>
         <td>TenD_Rate</td>
         %for i in xrange(bd,cd):
            %if ten_bsbz[i] < 10:
                <td>${ten_tr_list[i]}</td>
            %else:
                <td bgcolor="red">${ten_tr_list[i]}</td>
            %endif
         %endfor
         <td> </td>
     </tr>
     %endif


     </table>
